  "keywords": [
    "wordpress", "web", "backup", "backupper", "ftp", "sftp"
  ],
  "homepage": "https://github.com/Informatex-GmbH/WebBackupper",
  "license": "MIT",
  "version": "1.1.0",
  "authors": [
    {
      "name": "Informatex",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=8.2",
    "ext-zip": "*",
    "ext-ftp": "*",
    "ext-ssh2": "*",
    "ext-mbstring": "*"
  },
  "autoload": {
    "psr-4": {
      "ifmx\\WebBackupper\\": "src"
    }
  }
}
